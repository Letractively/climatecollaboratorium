<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>plansportlet</portlet-name>
		<icon>/icon.png</icon>
		<social-activity-interpreter-class>org.climatecollaboratorium.plans.activity.PlansActivityFeedEntry</social-activity-interpreter-class>
		<instanceable>true</instanceable>
		<header-portlet-css>/xmlhttp/css/xp/xp-portlet.css</header-portlet-css>
		<header-portlet-css>/xmlhttp/css/test.css</header-portlet-css>
		<header-portlet-css>/xmlhttp/css/simulation/simulation.css</header-portlet-css>
		<header-portlet-javascript>/js/plans-portlet.js</header-portlet-javascript>
		<header-portlet-javascript>/js/jquery.history.js</header-portlet-javascript>
		<header-portlet-javascript>/xmlhttp/js/simulation/model-inputs-render.js</header-portlet-javascript>
		<footer-portlet-css>/css/main.css</footer-portlet-css>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>