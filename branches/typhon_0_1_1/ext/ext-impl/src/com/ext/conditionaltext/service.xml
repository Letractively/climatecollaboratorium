<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="com.ext.conditionaltext">
	<namespace>inlinehelp</namespace>
	<entity name="ConditionalTextSetting" local-service="true">
	<!-- PK field -->
		<column name="ConditionalTextSettingId" type="Long" primary="true" />
		<column name="styleClass" type="String" />
		<column name="paramKey" type="String" />
		<column name="paramValue" type="String" />
        <column name="html" type="String" />

	   <!-- Finder methods -->


		<finder name="paramKey" return-type="Collection">
			<finder-column name="paramKey" />
		</finder>
        <finder name="paramKeyParamValue" return-type="ConditionalTextSetting">
			<finder-column name="paramKey" />
            <finder-column name="paramValue" />
		</finder>
		
	</entity>
	
	</service-builder>