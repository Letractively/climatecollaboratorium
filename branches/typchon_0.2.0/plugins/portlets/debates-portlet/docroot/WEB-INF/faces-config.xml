<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!-- application related managed beans -->
	<application>
		<message-bundle>messages-override</message-bundle>
		<resource-bundle>
			<base-name>Language</base-name>
			<var>msgs</var>
		</resource-bundle>
		<view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
	</application>
	<factory/>
	<converter>
		<converter-id>debate.PositionList</converter-id>
		<converter-class>org.climatecollaboratorium.debates.converters.DebatePositionListConverter</converter-class>
	</converter>
	<managed-bean>
		<managed-bean-name>edit</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.PortletPreferencesManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>language</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.LanguageManagedBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>com.icesoft.faces.async.render.RenderManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resumeUploader</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.icefaces.FileUploadManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>themeDisplay</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.ThemeDisplayManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editedDebateCategory</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.EditDebateCategoryBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editDebateComment</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.EditDebateCommentBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>debateDetailsBean</property-name>
			<value>#{debateDetails}</value>
		</managed-property>
		<managed-property>
			<property-name>permissions</property-name>
			<value>#{permissions}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>editDebateItem</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.EditDebateItemBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>debateDetailsBean</property-name>
			<value>#{debateDetails}</value>
		</managed-property>
		<managed-property>
			<property-name>permissions</property-name>
			<value>#{permissions}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>commentsBean</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebateItemCommentsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>debateDetailsBean</property-name>
			<value>#{debateDetails}</value>
		</managed-property>
		<managed-property>
			<property-name>permissions</property-name>
			<value>#{permissions}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>addQuestion</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.AddQuestionBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>debateDetails</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebateDetailsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>portletNavigation</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.PortletNavigationBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>debateDetailsBean</property-name>
			<value>#{debateDetails}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>debateCategories</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebateCategoriesBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>categories</property-name>
			<list-entries>
				<value-class>org.climatecollaboratorium.debates.bean.backing.DebateCategoriesBean</value-class>
			</list-entries>
		</managed-property>
		<managed-property>
			<property-name>editCategoryBean</property-name>
			<value>#{editedDebateCategory}</value>
		</managed-property>
		<managed-property>
			<property-name>addQuestionBean</property-name>
			<value>#{addQuestion}</value>
		</managed-property>
		<managed-property>
			<property-name>debatePreferences</property-name>
			<value>#{preferences}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>debateCategory</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.support.DebateCategoryWrapper</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>permissions</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebatesPermissionsBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>suggestion</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebatesSuggestBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>debatesPreferences</property-name>
			<value>#{preferences}</value>
		</managed-property>
		<managed-property>
			<property-name>debateDetailsBean</property-name>
			<value>#{debateDetails}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>preferences</managed-bean-name>
		<managed-bean-class>org.climatecollaboratorium.debates.bean.backing.DebatesPreferencesBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
</faces-config>