<liferay-portlet-app>
	<portlet>
		<portlet-name>models</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
        <render-weight>1</render-weight>
        <ajaxable>false</ajaxable>
        
		<header-portlet-css>/css/models-portlet.css</header-portlet-css>
		<footer-portlet-javascript>/js/models-portlet.js</footer-portlet-javascript>
        <header-portlet-css>/xmlhttp/css/xp/xp-portlet.css</header-portlet-css>
        <header-portlet-javascript>/js/jqueryCompatibility.js</header-portlet-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
