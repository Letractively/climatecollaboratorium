<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 5.2.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_5_2_0.dtd">

<liferay-portlet-app>
	<portlet>
		<portlet-name>sharedcollabfacelets</portlet-name>
		<icon>/icon.png</icon>
		<social-activity-interpreter-class>org.climatecollaboratorium.facelets.discussions.activity.DiscussionActivityFeedEntry</social-activity-interpreter-class>
		<!-- <friendly-url-mapper-class>com.climatecollaboratorium.navigation.SharedFriendlyURLMapper</friendly-url-mapper-class>-->
		<instanceable>true</instanceable>
		<header-portlet-css>/xmlhttp/css/xp/xp-portlet.css</header-portlet-css>
		<header-portlet-css>/css/test.css</header-portlet-css>
		<header-portlet-css>/css/debates/debates.css</header-portlet-css>
		<header-portlet-css>/css/simulation/simulation.css</header-portlet-css>
		<header-portlet-css>/css/discussion/discussion.css</header-portlet-css>
		<header-portlet-javascript>/js/simulation/model-inputs-render.js</header-portlet-javascript>
		<header-portlet-javascript>/js/navigation/navigationManager.js</header-portlet-javascript>
		<header-portlet-javascript>/js/debates/debates.js</header-portlet-javascript>
	</portlet>
	<role-mapper>
		<role-name>administrator</role-name>
		<role-link>Administrator</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>guest</role-name>
		<role-link>Guest</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>power-user</role-name>
		<role-link>Power User</role-link>
	</role-mapper>
	<role-mapper>
		<role-name>user</role-name>
		<role-link>User</role-link>
	</role-mapper>
</liferay-portlet-app>