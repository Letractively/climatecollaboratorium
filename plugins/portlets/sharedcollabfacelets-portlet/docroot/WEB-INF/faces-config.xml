<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.2" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
	<!--REMOVE_FROM_SHARED_LIB_BEGIN-->
	<application>
		<view-handler>com.icesoft.faces.facelets.D2DFaceletViewHandler</view-handler>
	</application>
	<factory></factory>
	<managed-bean>
		<managed-bean-name>edit</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.PortletPreferencesManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>language</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.LanguageManagedBean</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>renderManager</managed-bean-name>
		<managed-bean-class>com.icesoft.faces.async.render.RenderManager</managed-bean-class>
		<managed-bean-scope>application</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>resumeUploader</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.icefaces.FileUploadManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>renderManager</property-name>
			<value>#{renderManager}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>themeDisplay</managed-bean-name>
		<managed-bean-class>com.liferay.util.bridges.jsf.common.ThemeDisplayManagedBean</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
    <managed-bean>
        <managed-bean-name>simulationBeanTest</managed-bean-name>
        <managed-bean-class>org.climatecollaboratorium.facelets.simulations.test.SimulationTestBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <!-- REMOVE_FROM_SHARED_LIB_END  -->
    <managed-bean>
        <managed-bean-name>eventBus</managed-bean-name>
        <managed-bean-class>org.climatecollaboratorium.events.EventBusImpl</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>simulationBean</managed-bean-name>
        <managed-bean-class>org.climatecollaboratorium.facelets.simulations.SimulationBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
        <managed-property>
            <property-name>eventBus</property-name>
            <value>#{eventBus}</value>
        </managed-property>
    </managed-bean>
	<navigation-rule>
		<navigation-case>
			<from-outcome>testTag</from-outcome>
			<to-view-id>/xhtml/showcase/testTag.iface</to-view-id>
		</navigation-case>
        <navigation-case>
            <from-outcome>simulationInputs</from-outcome>
            <to-view-id>/xhtml/showcase/simulationInputsTag.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>simulationOutputs</from-outcome>
            <to-view-id>/xhtml/showcase/simulationOutputsTag.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>simulation</from-outcome>
            <to-view-id>/xhtml/showcase/simulationTag.iface</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>/xhtml/view.iface</to-view-id>
        </navigation-case>
	</navigation-rule>
</faces-config>