<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>debates3</portlet-name>
		<display-name>MIT Climate Collaboratorium Debates Portlet (3)</display-name>
		<portlet-class>com.icesoft.faces.webapp.http.portlet.MainPortlet</portlet-class>
		<init-param>
			<name>com.icesoft.faces.EDIT</name>
			<value>/xhtml/edit.iface</value>
		</init-param>
		<init-param>
			<name>com.icesoft.faces.HELP</name>
			<value>/xhtml/help.iface</value>
		</init-param>
		<init-param>
			<name>com.icesoft.faces.VIEW</name>
			<value>/xhtml/debates.iface</value>
		</init-param>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>edit</portlet-mode>
			<portlet-mode>help</portlet-mode>
		</supports>
		<portlet-info>
			<title>MIT Climate Collaboratorium Debates Portlet</title>
			<short-title>Debates portlet</short-title>
			<keywords>Debates Collaboratorium</keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>