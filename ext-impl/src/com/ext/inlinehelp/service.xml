<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 5.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_5_2_0.dtd">

<service-builder package-path="com.ext.inlinehelp">
	<namespace>inlinehelp</namespace>
	<entity name="HelpUserSetting" local-service="true">
	<!-- PK field -->
		<column name="HelpUserSettingId" type="Long" primary="true" />
		<column name="userId" type="Long" />
		<column name="messageId" type="String" />
		<column name="visible" type="Boolean" />
				
	   <!-- Finder methods -->


		<finder name="UserIdMessageId" return-type="Collection">
			<finder-column name="userId" />
            <finder-column name="messageId" />
		</finder>
		
	</entity>
	
	</service-builder>